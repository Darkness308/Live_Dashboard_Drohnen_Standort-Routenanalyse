# MORPHEUS Dashboard Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Google Maps JavaScript API Key (für Maps JS)
# Get your API key from: https://console.cloud.google.com/google/maps-apis
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Optional: Google Maps Map ID für 3D Buildings
GOOGLE_MAPS_MAP_ID=

# CORS Allowed Origins
ALLOWED_ORIGINS=http://localhost:8000,http://localhost:8080

# Optional: Enable/disable specific features
ENABLE_HEATMAP=true
ENABLE_3D_VIEW=true
ENABLE_ROUTE_COMPARISON=true

# Language Settings (DE or EN)
DEFAULT_LANGUAGE=DE

